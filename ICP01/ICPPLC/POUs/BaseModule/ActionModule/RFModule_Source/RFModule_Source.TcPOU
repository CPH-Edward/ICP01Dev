    <Method Name="getErrID" Id="{966ffbc3-0ea7-44e0-9d9d-783ef3b7d3d9}">
      <Declaration><![CDATA[METHOD getErrID : UDINT
VAR_INPUT
	errID					:UDINT;//取值范围1~2
END_VAR
